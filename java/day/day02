package my.day02;

public class DataTypeEx01 {  //클래스 정의부
    public static void main(String[] args) {
        // 논리 자료형
        System.out.println("---논리형---");
        // 변수 선언 및 초기화
        boolean bool = true;
        // 변수 사용
        System.out.println(bool);
        // 변수 선언 및 초기화
        boolean bool2 = 5>10;
        System.out.println(bool2);


    }
} // 클래스의 끝
