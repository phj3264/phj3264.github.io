package my.day03;

import java.util.Scanner;
/*
    5층 건물이 있습니다. 1층 약국,2층 정형외과, 3층 피부과,4층 치과, 5층 헬스장
    층을 입력받아 해당 층이 어떤 곳인지 알려주는 코드를 작성.
 */
public class SwitchTest2 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("층을 입력하세요>>");
        int num = scanner.nextInt();

        switch (num){
            case 1:
                System.out.println("1층 약국입니다");
                break;
            case 2:
                System.out.println("2층 정형외과입니다.");
                break;
            case 3:
                System.out.println("3층 피부과입니다.");
                break;
            case 4:
                System.out.println("4층 치과입니다.");
                break;
            case 5:
                System.out.println("5층 헬스장입니다.");
                break;
            default:
                System.out.println("해당 층이 없습니다.");
                break;

        }
    }
}
